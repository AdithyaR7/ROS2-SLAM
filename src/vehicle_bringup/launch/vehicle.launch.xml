<launch>
   
    <!-- Gazebo and Robot urdf-->
    <include file="$(find-pkg-share vehicle_bringup)/launch/spawn_robot_house.launch.py"/>
    
    <!-- SLAM Toolbox -->
    <node pkg="slam_toolbox" exec="async_slam_toolbox_node" name="slam_toolbox">
        <param name="use_sim_time" value="true"/>
        <param name="base_frame" value="base_link"/>
        <param name="odom_frame" value="odom"/>
        <param name="map_frame" value="map"/>
    </node>
    
    <!-- Teleop Keyboard : launch in terminal manually to use-->
    <!-- <node pkg="teleop_twist_keyboard" exec="teleop_twist_keyboard" name="teleop"/> --> 

</launch>

